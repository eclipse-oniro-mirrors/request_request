{
    "process": "download_server",
    "systemability": [
        {
            "name": 3706,
            "libpath": "libdownload_server.dylib.so",
            "run-on-create": false,
            "distributed": false,
            "dump_level": 1,
            "recycle-strategy": "low-memory",
            "start-on-demand": {
                "commonevent": [
                    {
                        "name": "usual.event.CONNECTIVITY_CHANGE",
                        "value": "3"
                    },
                    {
                        "name": "usual.event.USER_REMOVED"
                    },
                    {
                        "name": "usual.event.USER_SWITCHED"
                    },
                    {
                        "name": "usual.event.USER_STARTED"
                    }
                ]
            }
        }
    ]
}